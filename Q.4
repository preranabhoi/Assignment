#include <stdio.h>

Struct State {

    char name[50];

    int population;

    float lit_rate;

    float income;

};



int main() {

        struct State st1;





    strcpy(st1.name, “Maharashtra”);

    St1.population = 1000000;

    St1.lit_rate = 5.8;

    St1.income = 6000.00;





    Printf(“State Name: %s\n”, st1.name);

    Printf(“Population: %d\n”, st1.population);

    Printf(“Literacy Rate: %.1f\n”, st1.lit_rate);

    Printf(“Income: $%.2f\n”, st1.income);



    Return 0;

}
